{
  "version": 2,
  "name": "gdq-schedule",
  "alias": "gdq-schedule.com",
  "builds": [
    {"src": "build/**", "use": "@now/static"},
    {"src": "data/**", "use": "@now/static"},
    {"src": "index.html", "use": "@now/static"}
  ],
  "routes": [
    {"src": "/data/(.*)", "dest": "/data/$1"},
    {"src": "/build/(.*)", "dest": "/data/$1"},
    {"src": "/(.*)", "dest": "index.html"}
  ]
}
